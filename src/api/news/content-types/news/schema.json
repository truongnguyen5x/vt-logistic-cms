{
  "kind": "collectionType",
  "collectionName": "newss",
  "info": {
    "singularName": "news",
    "pluralName": "newss",
    "displayName": "News",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "type": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "internal_news",
        "market_news",
        "specialized_news",
        "recruitment"
      ],
      "required": true,
      "default": "internal_news"
    },
    "featured_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "relevants": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::news.news",
      "inversedBy": "news"
    },
    "news": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::news.news",
      "mappedBy": "relevants"
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "versions": {
          "versioned": true
        },
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo"
    },
    "is_hot": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        },
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean"
    },
    "preview_content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "unique": true,
      "required": true
    },
    "page_view": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "default": "0"
    },
    "like": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "default": "0"
    },
    "dislike": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "default": "0"
    },
    "contents": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor",
      "required": true
    }
  }
}
